<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
    <name>form2db-app</name>
    <displayName>${project.name}</displayName>
    <description>${project.description}</description>
    <class>de.marvinkerkhoff.form2db.Form2db</class>
    <versionHandler>de.marvinkerkhoff.form2db.setup.Form2dbVersionHandler</versionHandler>
    <version>${project.version}</version>

    <components>
        <id>app-form2db</id>
    </components>

    <dependencies>
        <dependency>
            <name>ui-admincentral</name>
            <version>${magnoliaVersion}/*</version>
        </dependency>
        <dependency>
            <name>dam-jcr</name>
            <version>${magnolia.dam.version}/*</version>
        </dependency>
        <dependency>
            <name>form</name>
            <version>${magnoliaFormVersion}/*</version>
        </dependency>
    </dependencies>

    <repositories>
        <repository>
            <name>magnolia</name>
            <workspaces>
                <workspace>form2db</workspace>
            </workspaces>
            <nodeTypeFile>/mgnl-nodetypes/form2db-nodetypes.xml</nodeTypeFile>
        </repository>
    </repositories>

</module>